class ModelMetadataAdapter extends TypeAdapter<ModelMetadata> {
  @override
  final int typeId = 3;

  @override
  ModelMetadata read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return ModelMetadata(
      version: fields[0] as String,
      checksum: fields[1] as String,
      releaseDate: fields[2] as DateTime,
      quantization: fields[3] as QuantizationFormat,
    );
  }

  @override
  void write(BinaryWriter writer, ModelMetadata obj) {
    writer
      ..writeByte(4)
      ..writeByte(0)
      ..write(obj.version)
      ..writeByte(1)
      ..write(obj.checksum)
      ..writeByte(2)
      ..write(obj.releaseDate)
      ..writeByte(3)
      ..write(obj.quantization);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is ModelMetadataAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
